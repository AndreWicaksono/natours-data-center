    "dayjs": "^1.11.10",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.10.3",
    "slugify": "^1.6.6",
    "styled-components": "^6.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
